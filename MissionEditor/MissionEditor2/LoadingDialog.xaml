<Window x:Class="MissionEditor2.LoadingDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Loading" Height="75" Width="300" ResizeMode="NoResize" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <UniformGrid Rows="2" Columns="1">
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Name="textBlock">Loading...</TextBlock>
        <ProgressBar Name="ProgressBar" x:FieldModifier="public" IsIndeterminate="True" />
    </UniformGrid>
</Window>
